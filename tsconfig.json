{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "common/*": ["src/common/*"],
      "config/*": ["src/config/*"],
      "services/*": ["src/services/*"],
      "hooks/*": ["src/hooks/*"],
      "store/*": ["src/store/*"],
      "domains/*": ["src/domains/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
